FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #include "include/ABLConditions"

    north
    {
        type            slip;
    }

    east
    {
        type            calculated;
        value           uniform 0;
    }

    south
    {
        type            slip;
    }

    west
    {
        type            calculated;
        value           uniform 0;
    }

    terrain
    {
        type            nutkAtmRoughWallFunction;
        z0              $z0;
        value           uniform 0.0;
    }

    ground
    {
        type            calculated;
        value           uniform 0;
    }

    "proc.*"
    {
        type            processor;
    }
}


